    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "randomstring": "^1.3.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.196",
    "@types/morgan": "^1.9.4",
