    "concurrently": "^8.2.0",
    "prettier": "^3.0.0",
    "prisma": "^5.0.0",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.1.6"
  }
}
